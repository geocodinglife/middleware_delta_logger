<h1>Login</h1>
<%= form_tag admin_sessions_path do %>
  <p>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= text_field_tag :password %>
  </p>
  <p><p>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </p><%= submit_tag 'Log in' %></p>
<% end %>